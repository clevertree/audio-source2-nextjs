export default async function rootTrack(t) {
const {loadInstrument, startTrack, wait} = t;
t.currentInstrument=t.lead2;
t.waitTime=(1/4)/1.5;

t.lead=loadInstrument(require("./instruments/oscillator"), {})
t.lead2=loadInstrument(require("./instruments/oscillator"), {type:'square'})

t.lead.config.type='pulse';
t.lead.config.pulseWidth=0.5;
t.lead.config.envelop.mixer=1;
t.lead2.config.type='square';
t.lead.config.envelop.mixer=0.5;


t.wut='ohok';t.omfg='1230WUT2of'; t.rully='yup';t.bpm=120;
t.currentInstrument=t.lead;
startTrack('track1'); await wait(3/8);
t.currentInstrument=t.lead2;
startTrack('track1'); await wait(5/8); await wait((3)/1.5);


}

