module.exports=async function rootTrack(trackRenderer) {
	const {setVariable, loadInstrument, startTrack, wait} = trackRenderer;
	setVariable('currentInstrument', trackRenderer.lead2);
	setVariable('waitTime', (1/4)/1.5);

	setVariable('lead', loadInstrument("./instruments/oscillator",{wait:(1/8)*1.5}));
	setVariable('lead2', loadInstrument("./instruments/oscillator",{'type':'square'}));

	setVariable('lead.config.type', 'pulse');
	setVariable('lead.config.pulseWidth', 0.5);
	setVariable('lead.config.envelop.mixer', 1);
	setVariable('lead2.config.type', 'square');
	setVariable('lead.config.envelop.mixer', 0.5);


	setVariable('wut', 'ohok');	setVariable('omfg', '1230WUT2of'); 	setVariable('rully', 'yup');	setVariable('bpm', 120);
	setVariable('currentInstrument', trackRenderer.lead);
	startTrack(track1); 	await wait(3/8);
	setVariable('currentInstrument', trackRenderer.lead2);
	startTrack(track1); 	await wait(5/8); 	await wait((3)/1.5);


}

async function track1(trackRenderer) {
	const {playFrequency, wait} = trackRenderer;

	playFrequency('C5', (1/4)*1.5); 	await wait(1/4);
	playFrequency('C4', (1/4)*1.5, 0.8);	await wait(1/4);
	playFrequency('G4', 1/4); 	await wait(1/4);
	playFrequency('Eb4', (1/4)*1.5, 0.8); 	await wait(1/4);
	playFrequency('Eb5', (1/4)/1.5); 	await wait(1/4);
	playFrequency('F5', (1/4)/1.5); 	await wait(1/4);
	playFrequency('Eb5', (1/4)/1.5); 	await wait(1/4);
	playFrequency('D5', (1/4)/1.5); 	await wait(1/4);
	playFrequency('C5', (1/4)*1.5); 	await wait(1/4);
	playFrequency('C4', (1/4)*1.5, 0.8); 	await wait(1/4);
	playFrequency('G4', 1/4); 	await wait(1/4);
	playFrequency('Eb4', (1/4)*1.5, 0.8); 	await wait(1/4);
	playFrequency('D5', 1/4); 	await wait(1/4);
	playFrequency('C4', 1/4, .8); 	await wait(1/4);
	playFrequency('Bb4', 1/4); 	await wait(1/4);
}